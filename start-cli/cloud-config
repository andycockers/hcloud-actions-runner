#cloud-config
  runcmd:
    - cd $RUNNER_HOME
    - instance_id=$(cat /var/lib/cloud/data/instance-id)
    - sudo -u $RUNNER_USER ./config.sh --unattended $extra_flags --name $instance_id --url "https://github.com/$gh_repo" --token $runner_token --labels $instance_id
    - ./svc.sh install $RUNNER_USER
    - ./svc.sh start